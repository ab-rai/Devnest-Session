int main() {
    priority_queue<int> maxHeap;
    priority_queue<int,vector<int>,greater<int> > minHeap;
    vector<int> vect{4,6,7,3,1};
    vector<int> asc;
    vector<int> desc;
   for(int i=0;i<vect.size();i++)
    {
        int currNum=vect[i];
        cout<<"\nInserting "<<currNum<<endl;
        maxHeap.push(currNum);
        minHeap.push(currNum);
        cout<<"current top element in the maxHeap is:-"<<maxHeap.top()<<endl;
        cout<<"current top element in the minHeap is:-"<<minHeap.top()<<endl;
       
    }
    cout<<"All the inserted values in descending order is:- \n";
    while(!maxHeap.empty())
    {
        desc.push_back(maxHeap.top());
        asc.push_back(minHeap.top());
        maxHeap.pop();
        minHeap.pop();
        
    }
    cout<<"Ascending order is:-\n";
    for(int num:asc)
        cout<<num<<" , ";
    cout<<"\nDescending order is:-\n";
    for(int num:desc)
        cout<<num<<" , ";
    
}

/*
Output:-
Inserting 4
current top element in the maxHeap is:-4
current top element in the minHeap is:-4

Inserting 6
current top element in the maxHeap is:-6
current top element in the minHeap is:-4

Inserting 7
current top element in the maxHeap is:-7
current top element in the minHeap is:-4

Inserting 3
current top element in the maxHeap is:-7
current top element in the minHeap is:-3

Inserting 1
current top element in the maxHeap is:-7
current top element in the minHeap is:-1
All the inserted values in descending order is:- 
Ascending order is:-
1 , 3 , 4 , 6 , 7 , 
Descending order is:-
7 , 6 , 4 , 3 , 1 , 

*/




